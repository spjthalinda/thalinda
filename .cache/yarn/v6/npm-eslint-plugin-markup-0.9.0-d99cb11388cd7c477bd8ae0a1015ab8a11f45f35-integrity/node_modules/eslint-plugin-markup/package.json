{
  "name": "eslint-plugin-markup",
  "version": "0.9.0",
  "description": "An incredible ESLint plugin for all markup languages based on markuplint.",
  "repository": "git+https://github.com/rx-ts/eslint.git",
  "homepage": "https://github.com/rx-ts/eslint/blob/main/packages/markup",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.es2015.mjs",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.es2015.mjs",
    "require": "./lib/index.js"
  },
  "es2015": "./lib/index.es2015.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-plugin-htm",
    "eslint-plugin-html",
    "markuplint",
    "htmllint",
    "htmlhint",
    "html-lint",
    "html-hint",
    "linthtml"
  ],
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "dependencies": {
    "cosmiconfig": "^7.0.1",
    "eslint-plugin-utils": "^0.3.0",
    "markuplint": "^2.7.1",
    "synckit": "^0.7.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./lib/es*.js",
    "./lib/index.js"
  ]
}
